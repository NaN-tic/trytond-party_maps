<data>
    <xpath expr="/form/field[@name='subdivision']" position="after">
        <newline/>
        <group col="6" colspan="4" id="maps" yfill="1">
          <separator id="geocoder" string="Geocoder" colspan="6"/>
          <label name="map_place"/>
          <field name="map_place"/>
          <label name="latitude"/>
          <field name="latitude"/>
          <label name="longitude"/>
          <field name="longitude"/>
          <label name="map_url"/>
          <field name="map_url" widget="url"/>
          <button name="geocode" colspan="6"/>
        </group>
    </xpath>
</data>
